{
    "compilerOptions": {
      "allowJs": true,
      "strict": true,
      "declaration": true,
      "outDir": "dist",
      "rootDir": "src",
      "jsx": "react",
      "lib": ["DOM", "DOM.iterable", "es5", "es6", "ESNext"],
      "target": "es5",
      "module": "es2015",
      "moduleResolution": "node",
      "allowSyntheticDefaultImports": true
    },
    "include": ["src/**/*"]
  }
  